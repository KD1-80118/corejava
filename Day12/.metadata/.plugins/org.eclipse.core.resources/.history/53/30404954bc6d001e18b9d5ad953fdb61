/*Create a list of strings. Find the string with highest length using Collections.max().*/

package assignment3;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Iterator;
import java.util.List;

public class Q3Assignment {
  public static void main(String[] args)
  {
	  List<String> list1 = new ArrayList();
	  
	 list1.add("Ram");
	 list1.add("Shyam");
	 list1.add("Shree");
	 list1.add("Sarang");
	 
	 Iterator<String> itr  =list1.iterator ();
	 while(itr.hasNext()) {
			String ele = itr.next(); // throws ConcurrentModificationException after adding "X".
			System.out.println(ele);
			
//		while(itr.hasNext())
//		{    String ele1 = itr.next();
//			String maxVal = Collections.max(list1);
//			System.out.println("Max Val: " + maxVal);		
//		}
	 }

}
}
